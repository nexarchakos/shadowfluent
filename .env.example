# OpenAI API Configuration
# Get your API key from: https://platform.openai.com/api-keys
# Copy this file to .env and add your actual API key

VITE_OPENAI_API_KEY=your_openai_api_key_here

# ElevenLabs TTS Configuration (server-side)
# Get your API key from: https://elevenlabs.io/
# Voice IDs can be found in your ElevenLabs dashboard
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_DEFAULT=your_voice_id_here
ELEVENLABS_VOICE_MALE=your_male_voice_id_here
ELEVENLABS_VOICE_FEMALE=your_female_voice_id_here
# Optional per-accent voices (override gender/default when set)
ELEVENLABS_VOICE_BRITISH_MALE=your_voice_id_here
ELEVENLABS_VOICE_AMERICAN_MALE=your_voice_id_here
ELEVENLABS_VOICE_AUSTRALIAN_MALE=your_voice_id_here
ELEVENLABS_VOICE_IRISH_MALE=your_voice_id_here
ELEVENLABS_VOICE_CANADIAN_MALE=your_voice_id_here
ELEVENLABS_VOICE_BRITISH_FEMALE=your_voice_id_here
ELEVENLABS_VOICE_AMERICAN_FEMALE=your_voice_id_here
ELEVENLABS_VOICE_AUSTRALIAN_FEMALE=your_voice_id_here
ELEVENLABS_VOICE_IRISH_FEMALE=your_voice_id_here
ELEVENLABS_VOICE_CANADIAN_FEMALE=your_voice_id_here
# Optional model override (default: eleven_flash_v2)
ELEVENLABS_MODEL=eleven_flash_v2
# Client flag to enable ElevenLabs via proxy
VITE_ELEVENLABS_PROXY_ENABLED=true
# Optional proxy URL override (default: /api/elevenlabs-tts)
VITE_ELEVENLABS_PROXY_URL=/api/elevenlabs-tts

# Optional: direct client mode for local dev (not for production)
# When VITE_ELEVENLABS_PROXY_ENABLED=false, the client will call ElevenLabs directly.
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
VITE_ELEVENLABS_VOICE_DEFAULT=your_voice_id_here
VITE_ELEVENLABS_VOICE_MALE=your_male_voice_id_here
VITE_ELEVENLABS_VOICE_FEMALE=your_female_voice_id_here
# Optional per-accent voices (override gender/default when set)
VITE_ELEVENLABS_VOICE_BRITISH_MALE=your_voice_id_here
VITE_ELEVENLABS_VOICE_AMERICAN_MALE=your_voice_id_here
VITE_ELEVENLABS_VOICE_AUSTRALIAN_MALE=your_voice_id_here
VITE_ELEVENLABS_VOICE_IRISH_MALE=your_voice_id_here
VITE_ELEVENLABS_VOICE_CANADIAN_MALE=your_voice_id_here
VITE_ELEVENLABS_VOICE_BRITISH_FEMALE=your_voice_id_here
VITE_ELEVENLABS_VOICE_AMERICAN_FEMALE=your_voice_id_here
VITE_ELEVENLABS_VOICE_AUSTRALIAN_FEMALE=your_voice_id_here
VITE_ELEVENLABS_VOICE_IRISH_FEMALE=your_voice_id_here
VITE_ELEVENLABS_VOICE_CANADIAN_FEMALE=your_voice_id_here
VITE_ELEVENLABS_MODEL=eleven_flash_v2

# Google Tag Manager Configuration
# Get your GTM Container ID from: https://tagmanager.google.com/
# Format: GTM-XXXXXXX (e.g., GTM-ABC1234)

VITE_GTM_CONTAINER_ID=GTM-XXXXXXX
